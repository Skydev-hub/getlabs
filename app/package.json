  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "dep-graph": "nx dep-graph",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "generate:svg-sprite": "svg-sprite -s --symbol-sprite='icons.svg' --symbol-dest=libs/shared/assets/src/assets/ --shape-id-generator='%s' libs/shared/assets/src/assets/icons/*.svg",
    "help": "nx help",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "docker-compose:env": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 BUILDKIT_PROGRESS=plain VERSION=$(git rev-parse HEAD) TAG=$(git rev-parse --short HEAD) docker-compose",
    "app:build": "ng run app:build",
    "app:docker:build": "yarn docker-compose:env build app",
    "app:docker:push": "yarn docker-compose:env push app",
    "app:docker:run": "yarn docker-compose:env run --service-ports app",
    "app:serve": "ng run app:serve",
    "web:build": "ng run web:build",
    "web:docker:build": "yarn docker-compose:env build web",
    "web:docker:push": "yarn docker-compose:env push web",
    "web:docker:run": "yarn docker-compose:env run --service-ports web",
    "web:serve": "ng run web:serve",
    "web:ssr:build": "run-p web:ssr:build:*",
    "web:ssr:build:browser-bundle": "ng run web:build",
    "web:ssr:build:server-bundle": "ng run web:build-server-bundle",
    "web:ssr:serve": "ng run web:serve-ssr",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "workspace-generator": "nx workspace-generator"
  },
