  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,scss}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "@getlabs": {
    "build": {
      "web": {
        "target": "ssr",
        "build-args": "build_type=ssr"
      }
    }
  }
